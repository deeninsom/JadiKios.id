generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model User {
  id           String    @id
  username     String
  no_telephone String    @unique
  password     String
  role         User_role @default(UMKM)
  createdAt    DateTime  @default(now())
  updatedAt    DateTime
}

enum User_role {
  ADMIN
  UMKM
  CUSTOMER
}
